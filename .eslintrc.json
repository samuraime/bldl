{
  "env": {
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": [
      2,
      {
        "ignore": [
          "got"
        ]
      }
    ]
  }
}